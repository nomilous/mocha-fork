{
  "name": "mocha-spawn",
  "version": "3.0.0",
  "description": "Mocha hooks to start/stop background local or remote proccesses in tests.",
  "engines": {
    "node": ">=4"
  },
  "main": "index.js",
  "bin": {
    "mocha-spawn-agent": "bin/mocha-spawn-agent"
  },
  "scripts": {
    "test": "mocha test/integration/*.js test/unit/*.js",
    "cover": "istanbul cover _mocha -- test/integration/*.js test/unit/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomilous/mocha-spawn.git"
  },
  "keywords": [
    "mocha",
    "fork",
    "background",
    "process"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomilous/mocha-spawn/issues"
  },
  "homepage": "https://github.com/nomilous/mocha-spawn#readme",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "expect.js": "^0.3.1",
    "fetch": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "semver": "^5.3.0"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "debug": "^3.1.0",
    "pem": "^1.12.3",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  }
}
